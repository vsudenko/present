  <DataSets>
    <DataSet Name="Backups">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>set transaction isolation level read uncommitted
set lock_timeout 10000

select type.typename
         , max_backup_date
from  (select 'I' type, 'Differential Database' typename union all
        select 'D' , 'Full Database' union all
        select 'F' , 'Filegroup Or Filegroup' union all
        select 'G' , 'Differential File' union all
        select 'P' , 'Partial' union all
        select 'Q' , 'Differential partial' union all
        select 'L' , 'Log' ) type
left join (
select type
       , max(backup_finish_date) max_backup_date
   from msdb.dbo.backupset 
where database_name = @db
group by type) agg on agg.type = type.type</CommandText>
        <QueryParameters>
          <QueryParameter Name="@db">
            <Value>=Parameters!DatabaseName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>tempdb</DataSourceName>
      </Query>
      <Fields>
        <Field Name="typename">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>typename</DataField>
        </Field>
        <Field Name="max_backup_date">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>max_backup_date</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="FileSize">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>set transaction isolation level read uncommitted
set lock_timeout 10000
SET FMTONLY OFF

create table #data(Fileid       bigint NOT NULL,
                   [FileGroup]  bigint NOT NULL,
                   TotalExtents bigint NOT NULL,
                   UsedExtents  bigint NOT NULL,
                   [Name]       sysname NOT NULL,
                   [FileName]   varchar(300) NOT NULL)


create table #log(dbname       sysname NOT NULL,
                  LogSize      numeric(15,7) NOT NULL,
                  LogUsed      numeric(9,5) NOT NULL,
                  Status       bigint NOT NULL)


insert #data exec('DBCC showfilestats with no_infomsgs')
insert #log exec('dbcc sqlperf(logspace) with no_infomsgs')

select 'Data:' as [Type],[Name],(TotalExtents*64)/1024.0 as [TotalMB],(UsedExtents*64)/1024.0 as [UsedMB]
from #data 
union all
select 'Transaction Log space:',db_name()+' LOG',LogSize,((LogUsed/100)*LogSize) from #log where dbname = db_name()
order by [Type],[Name]

drop table #data
drop table #log</CommandText>
        <DataSourceName>tempdb</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Type">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Type</DataField>
        </Field>
        <Field Name="Name">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Name</DataField>
        </Field>
        <Field Name="TotalMB">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>TotalMB</DataField>
        </Field>
        <Field Name="UsedMB">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>UsedMB</DataField>
        </Field>
        <Field Name="FreeMB">
          <Value>=Fields!TotalMB.Value-Fields!UsedMB.Value</Value>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DatabaseProperties">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>set transaction isolation level read uncommitted
set lock_timeout 10000

select suser_sname(d.owner_sid) as [Owner] ,d.create_date as [crdate],d.compatibility_level as [cmptlevel]
,d.collation_name as [Collation],d.user_access_desc as [UserAccess],
d.state_desc as [Status],d.recovery_model_desc as [Recovery]
from sys.databases d
where d.[name] = @db</CommandText>
